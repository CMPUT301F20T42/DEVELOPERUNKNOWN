<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fui_bgAnonymous">

    <ImageButton
        android:id="@+id/editImageButton"
        android:layout_width="118dp"
        android:layout_height="88dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="30dp"
        android:clickable="false"
        android:src="@drawable/defaultphoto"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchUnameEdit" />

    <TextView
        android:id="@+id/userFirstName"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.435"
        tools:layout_editor_absoluteX="184dp" />

    <EditText
        android:id="@+id/editContactPhone"
        android:layout_width="200dp"
        android:layout_height="38dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="phone"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.671"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/editContactEmail" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="44dp"
        android:text="FirstName:"
        android:textSize="15sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/userFirstName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/confirmEditProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="93dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="84dp"
        android:text="Confirm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editInfo"
        app:layout_constraintTop_toBottomOf="@+id/editContactPhone" />

    <EditText
        android:id="@+id/editContactEmail"
        android:layout_width="200dp"
        android:layout_height="38dp"
        android:layout_marginTop="20dp"
        android:ems="10"

        android:inputType="textEmailAddress"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.671"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/userLastName" />

    <Button
        android:id="@+id/editInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="48dp"
        android:text="Edit"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editContactPhone" />

    <TextView
        android:id="@+id/userName"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="243dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="44dp"
        android:text="LastName:"
        android:textSize="15sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/userLastName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/userEmail"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:textSize="10sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/userName"
        tools:layout_editor_absoluteX="307dp" />

    <TextView
        android:id="@+id/userLastName"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/userFirstName" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="44dp"
        android:text="Contact phone:"

        android:textSize="15sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editContactPhone"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginStart="44dp"
        android:text="Contact email:"
        android:textSize="15sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editContactEmail"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginTop="236dp"
        android:text="username:"
        android:textSize="15sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="63dp" />

    <EditText
        android:id="@+id/searchUnameEdit"
        android:layout_width="324dp"
        android:layout_height="56dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:hint="Enter the user you want to know"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/searchUserButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/searchUnameEdit"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
